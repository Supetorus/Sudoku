<Application x:Class="Sudoku.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="clr-namespace:Sudoku"
			 xmlns:system="clr-namespace:System;assembly=System.Runtime"
			 StartupUri="GameWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
                <local:SkinResourceDictionary DefaultSource="Styles/DefaultStyle.xaml"
											  DarkSource="Styles/DarkStyle.xaml"
											  NeonSource="Styles/NeonStyle.xaml"
                                              MochaSource="Styles/MochaStyle.xaml"
                                              />
            </ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
